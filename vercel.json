{
  "functions": {
    "src/pages/**/*.astro": {
      "runtime": "@vercel/static"
    }
  },
  "headers": [
    {
      "source": "/public/(.*).svg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        }
      ]
    },
    {
      "source": "/(.*).svg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
